//Business Rule to clear variable value(s) after record producer form submission
//When: Before
//Insert and Update
//Table: Medical Incident
//Condition: current.variable_pool.<variable name> !="" || current.variable_pool.<variable name> !=""

(function executeRule(current, previous /*null when async*) {

  //Add each variable here and match it to the condition line
  current.variable_pool.<variable name> = "";
  current.variable_pool.<variable name> = "";
  })(current, previous);
